{
  "name": "@nuclearredeye/eslint-config",
  "version": "1.1.3",
  "description": "ESLint config to enforce my preferred code style/quality preferences",
  "main": "./src/index.js",
  "files": [
    "src/**/*"
  ],
  "scripts": {
    "build": "echo \"Nothing to do for target 'build'\"",
    "lint": "echo \"Nothing to do for target 'lint'\"",
    "test": "echo \"Nothing to do for target 'test'\"",
    "release": "standard-version"
  },
  "keywords": [],
  "author": "NuclearRedeye",
  "license": "MIT",
  "homepage": "https://github.com/nuclearredeye/eslint-config#readme",
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "peerDependencies": {
    "eslint": "^7.24.0",
    "prettier": "^2.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "husky": "^4.2.1",
    "standard-version": "^7.1.0"
  },
  "dependencies": {
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0"
  }
}
